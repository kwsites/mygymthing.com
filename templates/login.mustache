<!doctype html>
<html>
<head>
    {{>meta}}
</head>

<body>
<nav class="social-nav">
    <ul>
        {{>social-nav-items}}
    </ul>
</nav>
<header>
    <div class="header-content">
        <form action="/auth/login" method="post" class="block-layout-form">
            <label><span>username</span> <input name="username" autofocus></label>
            <label><span>password</span> <input name="password" type="password"></label>

            <div class="spaced">
                <button data-action="/auth/login">log in</button> or
                <button data-action="/auth/register">sign up</button>
            </div>
        </form>
    </div>
</header>
<footer>my gym thing</footer>

{{>require}}
<script data-main="login" src="/{{ version }}/js/ext/require.2.1.6-min.js" async defer></script>
</body>
</html>
